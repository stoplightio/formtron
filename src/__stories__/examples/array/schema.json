{
  "$schema": "../../../../ui-schema.json",
  "title": "Arrays",
  "description": "Several types of arrays",
  "type": "form",
  "columns": 2,
  "fields": {
    "paths.*.*.tags": {
      "type": "array",
      "title": "Strings",
      "default": "",
      "items": {
        "title": "Tag name",
        "type": "string"
      }
    },
    "paths.*.*.numbers": {
      "type": "array",
      "title": "Integers",
      "default": 0,
      "items": {
        "title": "Number",
        "type": "integer"
      }
    },
    "paths.*.*.schemes": {
      "type": "array",
      "title": "Multiselects",
      "default": [],
      "span": 2,
      "items": {
        "title": "Scheme",
        "type": "multiselect",
        "options": ["http", "https", "ws", "wss"]
      }
    }
  }
}
