{
  "_selection": "securityDefinitions.petstore_auth",
  "securityDefinitions": {
      "petstore_auth": {
          "type": "oauth2",
          "authorizationUrl": "https://petstore.swagger.io/oauth/dialog",
          "flow": "implicit",
          "scopes": {
              "write:pets": "modify pets in your account",
              "read:pets": "read your pets"
          }
      },
      "api_key": {
          "type": "apiKey",
          "name": "api_key",
          "in": "header"
      }
  }
}
